@{
}
@model IEnumerable<string>
<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-projectType="" asp-route-pageNum ="1" class="btn btn-outline-secondary">Home</a>


    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-pageNum ="1"
           class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")"
           asp-route-projectType="@t">@t</a>
    }

</div>